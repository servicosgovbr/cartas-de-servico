<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<servico xmlns="http://servicos.gov.br/v3/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://servicos.gov.br/v3/schema ../servico.xsd">
    <nome><![CDATA[Pronatec Comunicações]]></nome>
    <sigla/>
    <nomes-populares/>
    <descricao><![CDATA[Oferta de cursos técnicos e profissionais e de formação inicial e continuada na área de tecnologias da informação e comunicação (TICs).]]></descricao>
    <contato/>
    <gratuito>true</gratuito>
    <solicitantes>
        <solicitante>
            <tipo>Qualquer cidadão</tipo>
            <requisitos><![CDATA[Cada curso exige requisitos específicos, tais como formação/ escolaridade anterior.]]></requisitos>
        </solicitante>
    </solicitantes>
    <tempo-total-estimado>
        <entre max="6" min="3" unidade="meses"/>
        <descricao><![CDATA[A oferta de vagas depende da disponibilização pela rede ofertante e da articulação realizada com os gestores municipais.]]></descricao>
    </tempo-total-estimado>
    <etapas>
        <etapa>
            <titulo>Inscrição</titulo>
            <descricao><![CDATA[Acessar o link do Pronatec e realizar a inscrição no curso desejado.]]></descricao>
            <documentos>
                <default>
                    <item>Documentação pessoal (CPF),</item>
                    <item>Certificado de conclusão de escolaridade (conforme requisito de cada curso)</item>
                    <item>Carteira de identidade</item>
                    <item>Título de eleitor</item>
                </default>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao tipo="web-inscrever-se">
                        <descricao><![CDATA[http://pronatec.mec.gov.br/inscricao/]]></descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
    </etapas>
    <orgao id="http://estruturaorganizacional.dados.gov.br/id/unidade-organizacional/1988"/>
    <segmentos-da-sociedade>
        <item>Cidadãos</item>
    </segmentos-da-sociedade>
    <areas-de-interesse>
        <item>Educação</item>
        <item>Educação profissionalizante</item>
        <item>Comunicações</item>
    </areas-de-interesse>
    <palavras-chave>
        <item>Educação profissionalizante</item>
        <item>Cursos FIC</item>
        <item>Pronatec Comunicações</item>
    </palavras-chave>
    <legislacoes>
        <item><![CDATA[Portaria MEC 168 de 07 de março de 2013.]]></item>
    </legislacoes>
</servico>
