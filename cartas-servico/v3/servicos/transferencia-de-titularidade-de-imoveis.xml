    <nome><![CDATA[Transferência de Titularidade de Imóveis]]></nome>
    <sigla/>
    <nomes-populares/>
    <descricao><![CDATA[Este serviço possibilita a realização de todos os procedimentos necessários à transferência de titularidade de imóvel da União sob os regimes de aforamento e de ocupação.]]></descricao>
    <contato/>
    <gratuito>false</gratuito>
    <solicitantes>
        <solicitante>
            <tipo>Pessoa física ou jurídica</tipo>
            <requisitos><![CDATA[Que pretenda efetuar a transferência de imóvel da União.]]></requisitos>
        </solicitante>
    </solicitantes>
    <tempo-total-estimado>
        <ate max="0" unidade="meses"/>
        <descricao><![CDATA[Não há prazo estipulado]]></descricao>
    </tempo-total-estimado>
    <etapas>
        <etapa>
            <titulo>Siga as etapas do processo</titulo>
            <descricao><![CDATA[O procedimento para a transferência de titularidade de imóvel da União, sob regime de ocupação ou aforamento, é realizado da seguinte forma:

Calcule o laudêmio, Certidão de Autorização para Transferência (CAT), Requerimento de Transferência de Titularidade de Imóvel]]></descricao>
            <documentos>
                <default/>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao tipo="web">
                        <descricao><![CDATA[http://e-spu.planejamento.gov.br/#/conteudo/33]]></descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
        <etapa>
            <titulo>Calcule o Laudêmio</titulo>
            <descricao><![CDATA[Calcule o laudêmio e emita o DARF para pagamento (1º passo para transferência de titularidade de imóvel).]]></descricao>
            <documentos>
                <default/>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao tipo="web">
                        <descricao><![CDATA[http://e-spu.planejamento.gov.br/#/conteudo/1]]></descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
        <etapa>
            <titulo>Emita a Certidão de Autorização para Transferência (CAT)</titulo>
            <descricao><![CDATA[Solicite a emissão da Certidão de Autorização para Transferência de Titularidade - CAT (2º passo para transferência de titularidade de imóvel)]]></descricao>
            <documentos>
                <default/>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao tipo="web">
                        <descricao><![CDATA[http://e-spu.planejamento.gov.br/#/conteudo/17]]></descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
        <etapa>
            <titulo>Requerer a Transferência de Titularidade de Imóvel</titulo>
            <descricao><![CDATA[Solicite a alteração do responsável/titular de imóvel da União no cadastro da SPU (3º e último passo para transferência de titularidade de imóvel)]]></descricao>
            <documentos>
                <default/>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao tipo="web">
                        <descricao><![CDATA[http://e-spu.planejamento.gov.br/#/requerimentoTransferenciaTitular?servico=14]]></descricao>
                    </canal-de-prestacao>
                </default>
