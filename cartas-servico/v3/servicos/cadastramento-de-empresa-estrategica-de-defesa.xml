        <item>Compras governamentais</item>
    </areas-de-interesse>
    <palavras-chave>
        <item>empresa</item>
        <item>estratégia</item>
        <item>defesa</item>
        <item>produtos</item>
    </palavras-chave>
    <legislacoes>
        <item><![CDATA[Lei 12598/2013]]></item>
    </legislacoes>
