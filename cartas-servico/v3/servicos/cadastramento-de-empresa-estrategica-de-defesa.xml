<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<servico xmlns="http://servicos.gov.br/v3/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://servicos.gov.br/v3/schema ../servico.xsd">
    <nome><![CDATA[Cadastramento de Empresa Estratégica de Defesa]]></nome>
    <sigla/>
    <nomes-populares/>
    <descricao><![CDATA[Certificação da empresa como de defesa ou estratégia de defesa.]]></descricao>
    <contato><![CDATA[http://www.defesa.gov.br/industria-de-defesa]]></contato>
    <gratuito>true</gratuito>
    <solicitantes>
        <solicitante>
            <tipo>Empresas candidatas</tipo>
            <requisitos><![CDATA[Produzir bem ou serviço de interesse das Forças Armadas]]></requisitos>
        </solicitante>
    </solicitantes>
    <tempo-total-estimado>
        <ate max="6" unidade="meses"/>
        <descricao/>
    </tempo-total-estimado>
    <etapas>
        <etapa>
            <titulo>Busca de Informações</titulo>
            <descricao><![CDATA[A empresa interessada pesquisa na internet.]]></descricao>
            <documentos>
                <default>
                    <item>Declaração de conteúdo nacional</item>
                </default>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao tipo="telefone">
                        <descricao/>
                    </canal-de-prestacao>
                    <canal-de-prestacao tipo="web">
                        <descricao/>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
        <etapa>
            <titulo>Ligação com o CECAFA</titulo>
            <descricao><![CDATA[A empresa interessada apresenta a documentação exigida ao CECAFA.]]></descricao>
            <documentos>
                <default>
                    <item>A cargo do CECAFA</item>
                </default>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao tipo="postal">
                        <descricao/>
                    </canal-de-prestacao>
                    <canal-de-prestacao tipo="web">
                        <descricao/>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
        <etapa>
            <titulo>Ligação com as Força Armada</titulo>
            <descricao><![CDATA[Empresa solicita apoio e aprovação da(s) Força(s)]]></descricao>
            <documentos>
                <default>
                    <item>A cargo da(s) Força(s) Armada(s)</item>
                </default>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao tipo="web">
                        <descricao/>
                    </canal-de-prestacao>
                    <canal-de-prestacao tipo="postal">
                        <descricao/>
                    </canal-de-prestacao>
                    <canal-de-prestacao tipo="presencial">
                        <descricao/>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
        <etapa>
            <titulo>Exame do produto pela comissão</titulo>
            <descricao><![CDATA[Em reuniões de especialistas e na plenária, os produtos são apreciados.]]></descricao>
            <documentos>
                <default>
                    <item>A cargo do CMID</item>
                </default>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao tipo="postal">
                        <descricao/>
                    </canal-de-prestacao>
                    <canal-de-prestacao tipo="web">
                        <descricao/>
                    </canal-de-prestacao>
                    <canal-de-prestacao tipo="presencial">
                        <descricao/>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
