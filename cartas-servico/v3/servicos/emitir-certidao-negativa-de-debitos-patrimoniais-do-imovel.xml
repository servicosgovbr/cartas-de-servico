<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<servico xmlns="http://servicos.gov.br/v3/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://servicos.gov.br/v3/schema ../servico.xsd">
    <nome><![CDATA[Emitir Certidão Negativa de Débitos Patrimoniais do Imóvel]]></nome>
    <sigla/>
    <nomes-populares>
        <item>Certidão de Imóvel</item>
    </nomes-populares>
    <descricao><![CDATA[Este serviço tem por objetivo agilizar e dar autonomia aos interessados em obter certidões de um imóvel de seu interesse.]]></descricao>
    <contato/>
    <gratuito>true</gratuito>
    <solicitantes>
        <solicitante>
            <tipo>Pessoa física ou jurídica</tipo>
            <requisitos><![CDATA[Que possua ou tenha possuído vínculo com a SPU em decorrência de utilização de imóvel da União (inscrição de ocupação, aforamento, concessão de direito real de uso - CDRU etc.).]]></requisitos>
        </solicitante>
    </solicitantes>
    <tempo-total-estimado>
        <ate max="0" unidade="minutos"/>
        <descricao/>
    </tempo-total-estimado>
    <etapas>
        <etapa>
            <titulo>Solicitar Certidão</titulo>
            <descricao><![CDATA[Solicitar a certidão na página da SPU]]></descricao>
            <documentos>
                <default>
                    <item>CPF/CPNJ</item>
                    <item>RIP do Imóvel</item>
                </default>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao tipo="web">
                        <descricao><![CDATA[http://atendimentovirtual.spu.planejamento.gov.br/Emissoes/Certidao/Cert_EmissPedido.asp]]></descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
    </etapas>
    <orgao id="http://estruturaorganizacional.dados.gov.br/id/unidade-organizacional/1986"/>
    <segmentos-da-sociedade>
        <item>Cidadãos</item>
        <item>Empresas</item>
    </segmentos-da-sociedade>
    <areas-de-interesse>
        <item>Habitação</item>
        <item>Patrimônio</item>
    </areas-de-interesse>
    <palavras-chave>
        <item>Certidão de Imóvel</item>
        <item>Patrimônio da União</item>
        <item>SPU</item>
    </palavras-chave>
    <legislacoes>
        <item><![CDATA[...]]></item>
    </legislacoes>
</servico>
