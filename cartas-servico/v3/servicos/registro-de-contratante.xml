        <item idSegmento="1" idServicoSegmento="2445">Cidadãos</item>
        <item idSegmento="4" idServicoSegmento="2446">Demais segmentos (ONGs, organizações sociais, etc)</item>
    </segmentos-da-sociedade>
    <areas-de-interesse>
