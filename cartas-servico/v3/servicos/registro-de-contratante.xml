        <item>Recursos humanos</item>
        <item>Relações de Trabalho</item>
        <item>Trabalho</item>
    </areas-de-interesse>
    <palavras-chave>
        <item id="1973">Técnicos em espetáculos de diversões</item>
        <item id="1974">artistas</item>
