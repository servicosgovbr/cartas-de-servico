<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<servico xmlns="http://servicos.gov.br/v3/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://servicos.gov.br/v3/schema ../servico.xsd">
    <nome><![CDATA[Consulta de Dados Cadastrais]]></nome>
    <sigla/>
    <nomes-populares>
        <item>Consulta de Dados Cadastrais</item>
    </nomes-populares>
    <descricao><![CDATA[Este serviço permite consultar os dados cadastrais do imóvel, suas benfeitorias e utilizações ativas, bem como as informações do seu responsável atual. Serão apresentadas informações gerais e técnicas de endereço, das testadas e do local de endereçamento das correspondências.]]></descricao>
    <contato/>
    <gratuito>true</gratuito>
    <solicitantes>
        <solicitante>
            <tipo>Pessoa física ou Jurídica</tipo>
            <requisitos><![CDATA[Que necessite verificar os dados cadastrais de um determinado imóvel.]]></requisitos>
        </solicitante>
    </solicitantes>
    <tempo-total-estimado>
        <ate max="0" unidade="minutos"/>
        <descricao><![CDATA[Não há prazo estipulado.]]></descricao>
    </tempo-total-estimado>
    <etapas>
        <etapa>
            <titulo>Acesse o Portal da SPU e selecione o requerimento desejado</titulo>
            <descricao><![CDATA[Acesse o Portal de Atendimento da SPU (http://e-spu.planejamento.gov.br/) e selecione o tipo de requerimento desejado.]]></descricao>
            <documentos>
                <default/>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao tipo="web">
                        <descricao><![CDATA[http://e-spu.planejamento.gov.br/#/conteudo/38]]></descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
        <etapa>
            <titulo>Preencha a informação solicitada</titulo>
            <descricao><![CDATA[Selecione a opção desejada (RIP SIAPA, CPF do Responsável, CNPJ do Responsável, Nome do Responsável) , preencha o campo e pressione o botão "OK".]]></descricao>
            <documentos>
                <default>
                    <item>RIP SIAPA, CPF do Responsável, CNPJ do Responsável ou Nome do Responsável</item>
                </default>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao tipo="web">
                        <descricao><![CDATA[http://e-spu.planejamento.gov.br/#/conteudo/38]]></descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
    </etapas>
    <orgao id="http://estruturaorganizacional.dados.gov.br/id/unidade-organizacional/1986"/>
    <segmentos-da-sociedade>
        <item>Cidadãos</item>
        <item>Empresas</item>
    </segmentos-da-sociedade>
    <areas-de-interesse>
        <item>Habitação</item>
        <item>Patrimônio</item>
    </areas-de-interesse>
    <palavras-chave>
        <item>Imóvel</item>
        <item>Informações</item>
        <item>Dados</item>
        <item>Cadastrais</item>
        <item>Consulta</item>
    </palavras-chave>
    <legislacoes>
        <item><![CDATA[Não há legislação vigente.]]></item>
    </legislacoes>
</servico>
