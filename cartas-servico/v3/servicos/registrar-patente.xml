        <etapa id="2907">
            <titulo>Fazer cadastro no Portal do INPI</titulo>
            <descricao>Após fazer o cadastro, guarde seu login e senha.</descricao>
            <documentos>
                <default>
                    <item id="3815">Dados de identificação e contato da pessoa física ou jurídica, conforme o caso</item>
                </default>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
