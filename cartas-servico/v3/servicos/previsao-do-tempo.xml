        <item>Agropecuária</item>
        <item>Águas</item>
        <item>Administração</item>
        <item>Água</item>
    </areas-de-interesse>
    <palavras-chave>
        <item>Palavras-Chave 01</item>
        <item>Palavras-Chave 02</item>
        <item>Palavras-Chave 03</item>
    </palavras-chave>
    <legislacoes>
        <item>www.planalto.gov.br</item>
        <item>www.planalto.gov.br/leis</item>
    </legislacoes>
</servico>
